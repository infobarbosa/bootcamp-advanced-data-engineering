{    
    "Name": "glue-job-pedidos-stream",
    "Description": "glue job de captura de dados de uma stream de pedidos",
    "ExecutionProperty": {
        "MaxConcurrentRuns": 1
    },
    "Command": {
        "Name": "gluestreaming",
        "PythonVersion": "3"
    },
    "DefaultArguments": {
        "--enable-metrics": "true",
        "--enable-spark-ui": "true",
        "--enable-job-insights": "true",
        "--enable-continuous-cloudwatch-log": "true",
        "--job-language": "python"
    },
    "MaxRetries": 0,
    "Timeout": 2880,
    "WorkerType": "G.1X",
    "NumberOfWorkers": 2,
    "GlueVersion": "4.0",
    "ExecutionClass": "STANDARD"
}